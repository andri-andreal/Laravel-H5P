{
    "name": "andreal/laravel-h5p",
    "description": "H5P Plugin for laravel platform",
    "type": "library",
    "authors": [
        {
            "name": "Andreal",
            "email": "info@andreal.net"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Andreal\\LaravelH5p\\": "src/LaravelH5p"
        }
    },
    "require": {
        "php": "^7.1",
        "laravel/framework": "5.7.*",
        "laravelcollective/html": "5.7",
         "h5p/h5p-core": "^1.24.0",
         "h5p/h5p-editor": "^1.24.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "1.2.*",
        "phpunit/phpunit": "~7.0",
        "xethron/migrations-generator": "^2.0"
    },

     "extra": {
        "laravel": {
            "providers": [
                "Andreal\\LaravelH5p\\LaravelH5pServiceProvider"
            ],
            "aliases": {
                "LaravelH5p": "Andreal\\LaravelH5p\\Facades"
            }
        }
    }

}
